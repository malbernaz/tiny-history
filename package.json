{
  "name": "tiny-history",
  "version": "2.0.5",
  "description": "A simple wrapper around the browser history API",
  "repository": "https://github.com/malbernaz/tiny-history.git",
  "author": "malbernaz <albernazmiguel@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/tiny-history.esm.js",
  "scripts": {
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "build": "npm run build:cjs && npm run build:esm && npm run build:iife",
    "build:cjs": "rollup -c rollup.config.js --output.format cjs --output.file dist/index.js",
    "build:esm":
      "rollup -c rollup.config.js --output.format es --output.file dist/tiny-history.esm.js",
    "build:iife":
      "rollup -c rollup.config.js --output.format iife --output.file dist/tiny-history.js",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.10.0",
    "expect": "^1.20.2",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^4.0.1",
    "promise-polyfill": "^6.0.2",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-uglify-es": "^0.0.1",
    "webpack": "^3.4.0"
  }
}
